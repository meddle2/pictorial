<div class="panel-heading"><p id="notice"><%= notice %></p></div>
<div class="panel-body">
<p>
  <%= image_tag @pin.image.url(:medium)%>
  <%= @pin.description %>
</p>
</div>
<div class="panel-footer">
  <% if @pin.user == current_user %>
    <%= link_to 'Edit', edit_pin_path(@pin), class: "btn btn-primary btn-xs"  %> |
  <% end %>
  <%= link_to 'Back', pins_path, class: "btn btn-warning btn-xs" %>
</div>